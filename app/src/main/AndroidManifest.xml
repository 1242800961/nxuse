<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileshopappcjb">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:name="com.example.mobileshopappcjb.common.MobileShopApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mobileshopappcjb.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mobileshopappcjb.activity.AdActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.MainActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.LoginActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.MyOrderActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.MyCollectActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.MyAddressActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.ChangePwdActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.GoodsListActivity"/>
        <activity android:name="com.example.mobileshopappcjb.activity.GoodsDetailActivity"/>
    </application>

</manifest>